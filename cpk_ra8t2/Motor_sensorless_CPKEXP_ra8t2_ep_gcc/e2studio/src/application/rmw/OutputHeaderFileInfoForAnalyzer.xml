<?xml version="1.0"?>
<OutputHeaderFileForAnalyzerSetting>
	<Header>/***********************************************************************************************************************</Header>
	<Header>* File Name    : {{CREATED FILENAME(r_mtr_control_parameter.h  )}}</Header>
	<Header>* Description  : Definition of default control parameters for sensorless speed control</Header>
	<Header>***********************************************************************************************************************/</Header>
	<Header>/* Guard against multiple inclusion */</Header>
	<Header>#ifndef R_MTR_CONTROL_PARAMETER_H</Header>
	<Header>#define R_MTR_CONTROL_PARAMETER_H</Header>
	<Header />
	<Header>/***********************************************************************************************************************</Header>
	<Header>* Macro definitions  </Header>
	<Header>***********************************************************************************************************************/</Header>
	<Header>#define    MTR_CONTROL_PARAMETER     (1)</Header>
	<Header />
	<Header>/* Target control parameter definitions */</Header>
	<OutputData Macro="CP_CURRENT_OMEGA" VariableName="com_f4_current_omega" Value="300.0f" Comment="/* Natural frequency for current loop */" />
	<OutputData Macro="CP_CURRENT_ZETA" VariableName="com_f4_current_zeta" Value="1.0f" Comment="/* Damping ratio for current loop */" />
	<OutputData Macro="CP_SPEED_OMEGA" VariableName="com_f4_speed_omega" Value="3.0f" Comment="/* Natural frequency for speed loop */" />
	<OutputData Macro="CP_SPEED_ZETA" VariableName="com_f4_speed_zeta" Value="1.0f" Comment="/* Damping ratio for speed loop */" />
	<OutputData Macro="CP_E_OBS_OMEGA" VariableName="com_f4_e_obs_omega" Value="1000.0f" Comment="/* Natural frequency of BEMF observer */" />
	<OutputData Macro="CP_E_OBS_ZETA" VariableName="com_f4_e_obs_zeta" Value="1.0f" Comment="/* Damping ratio of BEMF observer */" />
	<OutputData Macro="CP_PLL_EST_OMEGA" VariableName="com_f4_pll_est_omega" Value="20.0f" Comment="/* Natural frequency of PLL Speed estimate loop */" />
	<OutputData Macro="CP_PLL_EST_ZETA" VariableName="com_f4_pll_est_zeta" Value="1.0f" Comment="/* Damping ratio of PLL Speed estimate loop */" />
	<OutputData Macro="CP_ID_DOWN_SPEED_RPM" VariableName="com_f4_id_down_speed_rpm" Value="500" Comment="/* Speed to start decreasing id [rpm]  (mechanical) */" />
	<OutputData Macro="CP_ID_UP_SPEED_RPM" VariableName="com_f4_id_up_speed_rpm" Value="400" Comment="/* Speed to start increasing id [rpm]  (mechanical) */" />
	<OutputData Macro="CP_MAX_SPEED_RPM" VariableName="com_f4_max_speed_rpm" Value="2400" Comment="/* Maximum speed[rpm] (mechanical) */" />
	<OutputData Macro="CP_OVERSPEED_LIMIT_RPM" VariableName="com_f4_overspeed_limit_rpm" Value="4500" Comment="/* Over speed limit [rpm] (mechanical) */" />
	<OutputData Macro="CP_OL_ID_REF" VariableName="com_f4_ref_id" Value="0.3f" Comment="/* Id reference when low speed [A] */" />
	<Footer />
	<Footer>#endif /* R_MTR_CONTROL_PARAMETER_H */</Footer>
</OutputHeaderFileForAnalyzerSetting>